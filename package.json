{
  "name": "ns-web-core",
  "version": "1.0.1",
  "description": "This template should help get you started developing with Vue 3 and Typescript in Vite. The template uses Vue 3 `<script setup>` SFCs, check out the [script setup docs](https://v3.vuejs.org/api/sfc-script-setup.html#sfc-script-setup) to learn more.",
  "main": "index.js",
  "scripts": {
    "release": "bumpp package.json packages/*/package.json && pnpm publish -r",
    "build:ui": "gulp -f packages/components/script/build/gulpfile.ts",
    "build:utils": "cd packages/utils && pnpm run build",
    "build:http": "cd packages/http && pnpm run build",
    "build:all": "pnpm build:ui && pnpm build:utils && pnpm build:http"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@lchxiang/eslint-config": "^0.0.6",
    "@lchxiang/stylelint-config": "^1.0.5",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "@types/less": "^3.0.3",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-less": "^5.0.0",
    "less": "^4.1.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.45",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^9.10.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.2.3",
    "qs": "^6.11.0"
  }
}
